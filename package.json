{
  "name": "resume-angular",
  "version": "3.0.1",
  "description": "My Personal Resume site",
  "keywords": [
    "resume",
    "angular",
    "personal",
    "portfolio"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "eslint --ext .js,.ts ./src --fix",
    "deploy": "ng deploy --base-href=https://aakashgoplani.in/ --cname=aakashgoplani.in",
    "empty-emulator": "firebase emulators:start",
    "emulator": "firebase emulators:start --import test-data",
    "emulator-firestore": "firebase emulators:start --only firestore --import test-data",
    "export": "firebase emulators:export test-data",
    "local-dev": "firebase emulators:start --only firestore,auth,storage --import test-data",
    "docs": "compodoc -p tsconfig.docs.json -s -o -w --theme material",
    "server-build": "ng build --configuration production && http-server -c-1 --port 4100 ./dist/resume-angular/ -P http://localhost:4100?",
    "server": "http-server -c-1 --port 4100 ./dist/resume-angular/ -P http://localhost:4100?",
    "backup-firebase-db-json": "firestore-export --accountCredentials ./key.json --backupFile ./backup-db.json --prettyPrint",
    "import-from-firebase": "set FIRESTORE_EMULATOR_HOST=0.0.0.0:4080 && firestore-import --accountCredentials ./key.json --backupFile ./backup-db.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/fire": "^7.2.1",
    "@angular/forms": "~13.3.0",
    "@angular/material": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "~13.3.0",
    "firebase": "^9.6.8",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/eslint": "^8.2.1",
    "@types/eslint-scope": "^3.7.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "angular-cli-ghpages": "^1.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-ngrx": "^2.0.3",
    "firebase-tools": "^10.3.1",
    "http-server": "^14.1.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
